{
  "name": "electron-css",
  "version": "0.5.0",
  "description": "Easiest, framework agnostic Style in JS library",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "test-ci": "jest --coverage; cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "start": "webpack-serve",
    "build": "webpack -p",
    "prepublish": "npm run build; npm run test"
  },
  "serve": {
    "content": [
      "demo"
    ]
  },
  "jest": {
    "roots": [
      "src",
      "tests"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azukaar/electron-css.git"
  },
  "keywords": [
    "css",
    "css-in-js",
    "react",
    "angular"
  ],
  "author": "Yann Stepienik",
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/azukaar/electron-css/issues"
  },
  "homepage": "https://github.com/azukaar/electron-css#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^23.2.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "coveralls": "^3.0.1",
    "jest": "^23.2.0",
    "webpack": "^4.12.2",
    "webpack-serve": "^1.0.4",
    "webpack-cli": "^3.0.8"
  }
}
